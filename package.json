{
  "name": "@bjf5201/tsdown-config",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "description": "Custom, opininated config functions for tsdown build tool",
  "author": "Bethany Fannin <contact@bethanyfannin.com> (https://www.bethanyfannin.com)",
  "keywords": [
    "tdown",
    "preset"
  ],
  "license": "MIT",
  "homepage": "https://github.com/bjf5201/tsdown-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjf5201/tsdown-config"
  },
  "bugs": "https://github.com/bjf5201/tsdown-config/issues",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.19.0",
    "vscode": "^1.22.0"
  },
  "scripts": {
    "build": "tsdown",
    "ci": "pnpm lint && pnpm test --coverage && pnpm build",
    "dev": "tsdown --watch && pnpm run test",
    "fix": "echo \"Add lint:fix command (but called only fix for simplicity) via eslint and prettier\"",
    "lint": "echo \"Add lint command at later iteration via eslint and prettier\"",
    "prepare": "pnpm test && pnpm build",
    "release": "bumpp",
    "test": "pnpm run lint && pnpm run typecheck",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "unit": "vitest"
  },
  "devDependencies": {
    "@types/node": "25.0.10",
    "bumpp": "10.4.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.39.2",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "@arethetypeswrong/core": "0.18.2",
    "tsdown": "0.20.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
